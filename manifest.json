{
  "manifest_version": 3,
  "name": "leak",
  "version": "0.1",
  "description": "Leetcode 2 Anki extension.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost/"
  ],
  "action": {
      "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
